    "version": "0.2.0",
    "configurations": [
        {
            "name": "gomap wms proxy",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/main.go",
            "output": "debug.exe",
            "cwd": "${workspaceFolder}",
            "args": [
                "--config", "${workspaceFolder}/configs/config.yaml"
            ]
        },
        {
            "name": "gomap no config",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/main.go",
            "output": "debug.exe",
            "cwd": "${workspaceFolder}",
            "args": ["-i"]
        }
    ]
}